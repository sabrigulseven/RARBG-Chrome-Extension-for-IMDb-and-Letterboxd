{
    "name": "Rarbg Addon For Letterboxd",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://letterboxd.com/*"],
            "js": ["foreground.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://letterboxd.com/*"
    ],
    "permissions": [
        "tabs"
    ]
}